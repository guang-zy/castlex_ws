<launch>

    <!-- 启动相机节点  -->
    <!-- <include file="$(find astra_launch)/launch/astra.launch" /> -->
    <include file="$(find usb_cam)/launch/usb_cam-test.launch"/>


    <!-- 启动物体跟踪  -->
    <node pkg="castlex_vision_detector" name="astra_motion_detector" type="astra_motion_detector.py" output="screen">
        <!-- <remap from="input_rgb_image" to="/camera/rgb/image_raw" /> -->
        <remap from="input_rgb_image" to="/usb_cam/image_raw" />      
        <rosparam>
            minArea: 500
            threshold: 25
        </rosparam>
    </node>

    <!-- 启动订阅cv_bridge_image话题  -->
    <node name="rqt_image_view" pkg="rqt_image_view" type="rqt_image_view" args="/cv_bridge_image"/>

</launch>
